{
    "name": "mini-antui",
    "version": "1.0.18",
    "description": "小程序版AntUI",
    "repository": {
        "type": "git",
        "url": "git@github.com:ant-mini-program/mini-antui.git"
    },
    "files": ["es"],
    "keywords": ["antui", "mini-program"],
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.1.6",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/preset-env": "^7.1.6",
        "@babel/preset-typescript": "^7.1.0",
        "eslint": "^5.16.0",
        "eslint-config-ali": "^3.1.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-typescript": "^0.14.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.0.0",
        "gulp-if": "^2.0.2",
        "gulp-less": "^4.0.1",
        "gulp-rename": "^1.2.3",
        "lint-staged": "^8.2.0",
        "mini-types": "^0.0.1",
        "pre-commit": "^1.2.2",
        "stylelint": "^10.1.0",
        "stylelint-config-standard": "^18.2.0",
        "typescript": "^3.2.4",
        "typescript-eslint-parser": "^22.0.0"
    },
    "scripts": {
        "lint-staged": "lint-staged",
        "build": "yarn && cross-env NODE_ENV=dev node scripts/compiler.js",
        "pub": "npm run build && node scripts/pub.js",
        "dev": "yarn && node scripts/compiler.js"
    },
    "pre-commit": ["lint-staged"],
    "lint-staged": {
        "*.ts": ["eslint --ext .ts", "git add"],
        "*.js": ["eslint", "git add"],
        "*.less": ["stylelint --syntax less"]
    },
    "dependencies": { "cross-env": "^5.2.0" }
}
